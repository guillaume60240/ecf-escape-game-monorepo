{
    "name": "ecf-escape-game-mono-repo",
    "version": "1.0.0",
    "description": "Escape game",
    "private": true,
    "scripts": {
        "start": "cd apps/back-end-api && npm start",
        "test": "cd apps/back-end-api && npm test",
        "build": "cd apps/back-end-api && npm run build",
        "lint": "cd apps/back-end-api && npm run lint",
        "format": "cd apps/back-end-api && npm run format",
        "format:fix": "cd apps/back-end-api && npm run format:fix && cd apps/web-app && npm run lint",
        "install:packages": "cd apps/back-end-api && npm install"
    }
}